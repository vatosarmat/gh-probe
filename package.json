{
  "name": "gh-probe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:source_map": "react-scripts build",
    "clean": "rm -rf ./build",
    "postinstall": "patch-package && ( cd node_modules/redux-saga-test-plan && yarn && yarn build )",
    "lint": "eslint src",
    "start": "react-scripts start",
    "test": "DEBUG_PRINT_LIMIT=20000 react-scripts test",
    "test:coverage": "yarn test --coverage --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended"
    ],
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "import/no-anonymous-default-export": "off"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "/!node_modules\\/lodash-es/"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.6",
    "@types/parse-link-header": "^1.0.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "coveralls": "^3.1.1",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "mdi-material-ui": "^6.22.1",
    "parse-link-header": "^1.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-packagejson": "^2.2.13",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "vatosarmat/redux-saga-test-plan#de90d6b",
    "reselect": "^4.1.1",
    "source-map-explorer": "^2.5.2",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.4.4"
  }
}
