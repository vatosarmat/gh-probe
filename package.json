  "dependencies": {
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.7.1",
    "@types/parse-link-header": "^1.0.0",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.4",
    "coveralls": "^3.0.9",
    "dayjs": "^1.8.15",
    "lodash": "^4.17.15",
    "mdi-material-ui": "^6.4.1",
    "parse-link-header": "^1.0.1",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "vatosarmat/redux-saga-test-plan#build",
    "reselect": "^4.0.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "3.7.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "DEBUG_PRINT_LIMIT=20000 react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "test:coverage_report": "yarn test:coverage --coverageReporters=text-lcov | coveralls",
    "clean": "rm -rf ./build",
    "build": "GENERATE_SOURCEMAP=false react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "@typescript-eslint/no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
